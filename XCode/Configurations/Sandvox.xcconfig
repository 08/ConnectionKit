
// include SVN_VERSION (the first step in any Sandvox build)
#include "/private/tmp/build-sandvox/BuildSettings.xcconfig"

// the "marketing" version is baked into CFBundleShortVersionString
SANDVOX_VERSION = 2.1b1

// the SLA variant to wrap around the .dmg [ALPHA,BETA,RELEASE]
// Also determines if VARIANT_ALPHA or VARIANT_BETA or VARIANT_RELEASE is defined.
// For checking alpha/beta behaviors, better to test for being *not* VARIANT_RELEASE
// We use the macro with the secret key so that sparkle can match with key on the server end.
PRODUCT_VARIANT = BETA



// expiry for beta
EXPIRY_TIMESTAMP = '@\"2011-06-30 11:59:59 -0800\"'

// Release builds checkout and use these specific revisions


// iMedia

		/// FOR RELEASE, PLEASE TIE IMEDIA TO A PARTICULAR NUMBER. * * * * * * * * * * * * * * *


BDALIAS_COMMIT = 8ea89475440bdce1f6d880ddbdbed6f62e1e6f8d
CK_COMMIT = release-1.2
IMEDIA_REVISION = master
KSERROR_COMMIT = v1.1
KSEXTENSIBLEMANAGEDOBJECT_COMMIT = v2.0
KSFILEUTILS_COMMIT = v1.1.1
KSHTMLWRITER_COMMIT = v1.2
KSSELECTIONBORDER_COMMIT = v1.0
KSTHREADPROXY_COMMIT = v1.0
SHARED_COMMIT = master
YRKSPINNINGPROGRESSINDICATOR_COMMIT = fe71a17098f56e62e65b36da0d2ba639481e4899


// Building -- common settings across all configurations

SDKROOT = macosx10.6

// GCC 4.2
GCC_VERSION = 4.2

// Clang LLVM 1.0 (Waiting for one update to Clang to accept Class<...>)
// GCC_VERSION = com.apple.compilers.llvm.clang.1_0

