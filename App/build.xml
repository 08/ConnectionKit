<project name="Sandvox" default="build" basedir=".">
<target name="clean">
	<exec executable="xCodeBuild" failonerror="true">
		<arg line="-project Marvel.xcodeproj -target Sandvox-CompleteApplication -configuration Release clean"/>
	</exec>
</target>
<target name="build">
	<exec executable="xCodeBuild" failonerror="true">
		<arg line="-project Marvel.xcodeproj -target Sandvox-CompleteApplication -configuration Release build"/>
	</exec>
</target>
</project>
